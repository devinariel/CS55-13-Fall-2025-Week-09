    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "hosting": {
      "port": 5000
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json",
    "database": "(default)"
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "the-therapy-compass.firebasestorage.app"
  },
