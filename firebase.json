{
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "hosting": {
      "port": 5000
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json",
    "database": "(default)"
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "the-therapy-compass.firebasestorage.app"
  },
  "apphosting": [
    {
      "runConfig": {
        "secrets": [
          {
            "secret": "FIREBASE_API_KEY",
            "environmentVariable": "FIREBASE_API_KEY",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "FIREBASE_AUTH_DOMAIN",
            "environmentVariable": "FIREBASE_AUTH_DOMAIN",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "FIREBASE_PROJECT_ID",
            "environmentVariable": "FIREBASE_PROJECT_ID",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "FIREBASE_STORAGE_BUCKET",
            "environmentVariable": "FIREBASE_STORAGE_BUCKET",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "FIREBASE_MESSAGING_SENDER_ID",
            "environmentVariable": "FIREBASE_MESSAGING_SENDER_ID",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "FIREBASE_APP_ID",
            "environmentVariable": "FIREBASE_APP_ID",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "DATABASE_PASSWORD",
            "environmentVariable": "DATABASE_PASSWORD",
            "availability": ["RUNTIME"]
          },
          {
            "secret": "GEMINI_API_KEY",
            "environmentVariable": "GEMINI_API_KEY",
            "availability": ["RUNTIME"]
          }
        ]
      },
      "backendId": "thetherapycompass",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ],
      "alwaysDeployFromSource": false
    }
  ]
}
